#ifndef USART_HPP_
#define USART_HPP_

class USART {
	private:
        USART_TypeDef *usart;
        uint32_t baudRate;
	public:
        USART(USART_TypeDef usart, uint32_t baudrate)
            : usart(usart), baudRate(baudrate) {}
};



#endif /* USART_HPP_ */
